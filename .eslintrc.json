{
  "extends": [
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "allowImportExportEverwhere": true,
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ]
  }
}